<%- include('../partials/top') %>
    <!-- Preloader Starts -->
    <div class="preloader">
        <div class="spinner"></div>
    </div>
    <!-- Preloader End -->

   <%- include ('../partials/header') %>

   <!-- Banner Area Starts -->
    <%- include ('./banner') %>
   
    <!-- Banner Area End -->

        <!-- Feature Area Starts -->
        <section class="feature-area section-padding">
            <div class="container">
                <h2 class="text-center">COVID-19 Tally!</h2>

                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h4 class="text-center">District Yamunanagar CENSUS</h4>
                    </div>
                    <div class="col-lg-12 text-center">
                        <h4>Covid Tally Last Updated On : <%= new Date(updatedAt.toLocaleString()).toLocaleString("en-US", {timeZone: "Asia/Kolkata"}) %> </h4>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="single-feature text-center item-padding">
                            <h4>Total Sample Taken</h4>
                            <h3><%= total_sample_taken %></h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-feature text-center item-padding mt-4 mt-md-0">
                            <h4>Positive Reported</h4>
                            <h3><%= positive_reported %></h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-feature text-center item-padding mt-4 mt-lg-0">
                            <h4>Patient Under Treatment</h4>
                            <h3><%= patient_under_treatment %></h3>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-6">
                        <div class="single-feature text-center item-padding mt-4 mt-lg-0">
                            <h4>Negative Report</h4>
                            <h3><%= negative_reported %></h3>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    

    <!-- Welcome Area Starts -->
    <section class="welcome-area" style="margin-bottom: 50px;">
        <div class="container">
            
        <div class="row">
                <div class="col-lg-5 align-self-center">
                    <div class="welcome-img">
                        <img src="public/images/covid.jpg" alt="" style="border: 2px solid grey">
                    </div>
                </div>
                <div class="col-lg-7">
                    <div class="welcome-text mt-2 mt-lg-0">
                        <div class="appointment-form mt-2 mt-lg-0">
                            <h4>Please Isolate Yourself Till Corona Report Negative!</h4>
                            <h3 class="mb-5">Covid-19 Reports</h3>
                            <form action="/generate-report" method="POST">
                                <div class="form-group">
                                    <input type="text" placeholder="Enter Your SRF Number Here" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Enter your SRF Number here'" name="srfno" class="form-control border" required>
                                </div>
                                <input type="submit" class="template-btn" value="Generate Report">
                            </form>
                            <h4 class="mt-4">IMPORTANT : </h4>
                            <h5>1. Please wait for 24 to 48 hours for Corona Report.</h5>
                            <h5>   कोरोना रिपोर्ट के लिए कृपया 24 से 48 घंटे तक प्रतीक्षा करें।</h5>
                            <hr>
                            <h5>2. Isolate Yourself after giving sample till Results are Recieved.</h5>
                            <h5>   करोना टेस्ट के नतीजे आने तक खुद को आइसोलेट (अलग) रखें।</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
